]]

Tab:AddButton({
	Name = "ESP-PLAYER unremovable esp",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/FishExploitsYT/ESP/53c5835d974487dad9297200f630e58e45d69427/Good%20ESP.lua"))()
  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]

Tab:AddButton({
	Name = "NEW ESP TEST",
	Callback = function()
      		local player = game.Players.LocalPlayer
local enabled = false

local function toggleESP()
    enabled = not enabled
    if enabled then
        -- Show ESP
        for _, otherPlayer in ipairs(game.Players:GetPlayers()) do
            if otherPlayer ~= player then
                local part = Instance.new("Part")
                part.Name = "SkeletonESP"
                part.Anchored = true
                part.Transparency = 0.5
                part.Size = Vector3.new(5, 10, 5) -- Adjust the size as desired
                part.BrickColor = BrickColor.new("White") -- Adjust the color as desired
                part.Position = otherPlayer.Character.HumanoidRootPart.Position
                part.Parent = workspace
                local nameLabel = Instance.new("BillboardGui")
                nameLabel.Name = "NameLabel"
                nameLabel.AlwaysOnTop = true
                nameLabel.Size = UDim2.new(0, 100, 0, 20) -- Adjust the size as desired
                nameLabel.StudsOffset = Vector3.new(0, 3, 0) -- Adjust the offset as desired
                nameLabel.Adornee = part
                local textLabel = Instance.new("TextLabel")
                textLabel.Name = "TextLabel"
                textLabel.Size = UDim2.new(1, 0, 1, 0)
                textLabel.BackgroundTransparency = 1
                textLabel.TextColor3 = Color3.new(1, 1, 1) -- Adjust the text color as desired
                textLabel.TextStrokeTransparency = 0
                textLabel.Text = otherPlayer.Name
                textLabel.Font = Enum.Font.SourceSansBold -- Adjust the font as desired
                textLabel.Parent = nameLabel
                nameLabel.Parent = part
            end
        end
    else
        -- Hide ESP
        for _, part in ipairs(workspace:GetChildren()) do
            if part.Name == "SkeletonESP" then
                part:Destroy()
            end
        end
    end
end

game:GetService("UserInputService").InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.E then -- Change the keybind as desired
        toggleESP()
    end
end)

  	end    
})

